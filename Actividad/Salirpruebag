public class moto {
    private String placa;
    private double cilindraje;
    private Calendar horaEntrada;
    private Calendar horaSalida;
    private double valorPagar;
    private boolean estado;
    private String tipoMoto;

    public moto(String placa, int cilindraje, Calendar horaEntrada, Calendar horaSalida, double valorPagar, boolean estado, String tipoMoto) {
        this.placa = placa;
        this.cilindraje = cilindraje;
        this.horaEntrada = horaEntrada;
        this.horaSalida = horaSalida;
        this.valorPagar = valorPagar;
        this.estado = estado;
        this.tipoMoto = tipoMoto;
    }




}



if cilindraje>= 400
    System.out.printIn("La moto es de alto cilindraje, porfavor siga")
    
    else if
    System.out.printIn("La moto no cumple los requisistos para este lugar, estacioncese en el lugar de bajo cilindraje")